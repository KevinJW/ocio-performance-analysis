{
  "chart": {
    "figure_size": [
      12,
      8
    ],
    "dpi": 150,
    "style": "darkgrid",
    "color_palette": "husl",
    "font_size": 10,
    "title_font_size": 14,
    "label_font_size": 12,
    "format": "png",
    "bbox_inches": "tight",
    "transparent": false
  },
  "analysis": {
    "outlier_threshold": 2.0,
    "outlier_method": "zscore",
    "max_reasonable_time_ms": 100000.0,
    "min_reasonable_time_ms": 0.1,
    "max_missing_data_pct": 10.0,
    "required_columns": [
      "avg_time",
      "min_time",
      "max_time",
      "file_name",
      "cpu_model"
    ],
    "confidence_level": 0.95,
    "percentiles": [
      5,
      10,
      25,
      50,
      75,
      90,
      95,
      99
    ]
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file_logging": true,
    "console_logging": true,
    "log_file": "ocio_analysis.log",
    "max_file_size_mb": 10,
    "backup_count": 3
  },
  "default_data_dir": "data",
  "default_output_dir": "analysis_results",
  "enable_caching": true,
  "parallel_processing": false,
  "max_workers": 4
}